/**
 * 存放数据封装类
 */
package com.pactera.bean;

import java.io.Serializable;


/**
 * 邮件信息类
 * @author 金轶伦
 * @version 1.0
 * create:2013-06-06
 *
 */
public class Mailtb implements Serializable {
	private int mid;//邮件ID
	private String fromaddress;//发邮件地址
	private String frompassword;//发邮件密码
	private String toaddress;//收邮件地址

	/**
	 * 默认构造方法
	 */
	public Mailtb() {
		super();
	}
	
	/**
	 * 不带主键的构造方法
	 * @param fromaddress	发邮件地址
	 * @param frompassword	发邮件密码
	 * @param toaddress		收邮件地址
	 */
	public Mailtb(String fromaddress, String frompassword, String toaddress) {
		super();
		this.fromaddress = fromaddress;
		this.frompassword = frompassword;
		this.toaddress = toaddress;
	}
	
	/**
	 * 带所有参数的构造方法
	 * @param mid			邮件ID
	 * @param fromaddress	发邮件地址
	 * @param frompassword	发邮件密码
	 * @param toaddress		收邮件地址
	 */
	public Mailtb(int mid, String fromaddress, String frompassword,
			String toaddress) {
		super();
		this.mid = mid;
		this.fromaddress = fromaddress;
		this.frompassword = frompassword;
		this.toaddress = toaddress;
	}
	
	/**
	 * @return the mid
	 */
	public int getMid() {
		return mid;
	}
	
	/**
	 * @param mid the mid to set
	 */
	public void setMid(int mid) {
		this.mid = mid;
	}
	
	/**
	 * @return the fromaddress
	 */
	public String getFromaddress() {
		return fromaddress;
	}
	
	/**
	 * @param fromaddress the fromaddress to set
	 */
	public void setFromaddress(String fromaddress) {
		this.fromaddress = fromaddress;
	}
	
	/**
	 * @return the frompassword
	 */
	public String getFrompassword() {
		return frompassword;
	}
	
	/**
	 * @param frompassword the frompassword to set
	 */
	public void setFrompassword(String frompassword) {
		this.frompassword = frompassword;
	}
	
	/**
	 * @return the toaddress
	 */
	public String getToaddress() {
		return toaddress;
	}
	
	/**
	 * @param toaddress the toaddress to set
	 */
	public void setToaddress(String toaddress) {
		this.toaddress = toaddress;
	}
	
	
}
